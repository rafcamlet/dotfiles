{
  "vetur.trace.server": "verbose",
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": []
    },
    "lua": {
      "cwd": "/home/r2d2/src/lua-language-server",
      "command": "/home/r2d2/src/lua-language-server/bin/Linux/lua-language-server",
      "args": ["-E", "-e", "LANG=en", "/home/r2d2/src/lua-language-server/main.lua"],
      "filetypes": ["lua"],
      "rootPatterns": [".git/"],
      "settings": {
        "Lua": {
          "diagnostics": {
            "globals": ["vim", "describe", "it"],
            "disable": ["lowercase-global"]
          },
          "runtime": {
            "version": "LuaJIT"
          }
        }
      }
    }
  }
}
