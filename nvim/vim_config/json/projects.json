[
  {
    "name": "NvimSF",
    "path": "projects/nvim-sf"
  },
  {
    "name": "Customease",
    "path": "projects/customease",
    "options": {
      "path": ".,app/**,app/javascript,,",
      "suffixesadd": ".js,.vue",
      "includeexpr": "Snakecase(v:fname)",
      "include": "import "
    },
    "fzf": {
      "o": "app/",
      "a": "app/concepts",
      "j": "app/javascript"
    }
  },
  {
    "name": "Blog",
    "path": "projects/blog"
  },
  {
    "name": "Notes",
    "path": "Dropbox/notes",
    "options": {
      "filetype": "my-note",
      "syntax": "my-note"
    }
  },
  {
    "name": "Talebook",
    "path": "projects/talebook/frontend",
    "options": {
      "path": ".,src/**,,",
      "suffixesadd": ".js,.vue",
      "commentstring": "// %s",
      "includeexpr": "tolower(v:fname)"
    },
    "regex_jump": {
      "T": {
        "from": "src/(.*)\\.(js|vue)",
        "to": "test/unit/{}.js"
      }
    },
    "fzf": {
      "c": "src/components",
      "s": "src/services",
      "t": "src/stores"
    }
  },
  {
    "name": "Reactapp",
    "path": "projects/reactapp",
    "dictionary": "~/.config/nvim/dict/react",
    "options": {
      "path": ".,src/**,,",
      "suffixesadd": ".js"
    },
    "fzf": {
      "c": "src/components"
    }
  },
  {
    "name": "Ixl",
    "path": "projects/ixl-front-end",
    "dictionary": "~/.config/nvim/dict/react",
    "options": {
      "shiftwidth": 4,
      "softtabstop": 4,
      "tabstop": 4,
      "path": ".,,src/**",
      "suffixesadd": ".js"
    },
    "fzf": {
      "c": "src/components",
      "r": "src/redux",
      "v": "src/views"
    }
  }
]
