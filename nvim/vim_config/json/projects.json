[
  {
    "name": "NvimSF",
    "root_path": "projects/nvim-sf"
  },
  {
    "name": "Customeasy",
    "root_path": "projects/customease",
    "settings": {
    "path": ".,app/**,app/javascript,,",
    "suffixesadd": ".js,.vue"
    }
  },
  {
    "name": "Blog",
    "root_path": "projects/blog"
  },
  {
    "name": "Notes",
    "root_path": "Dropbox/notes",
    "settings": {
      "filetype": "my-note",
      "syntax": "my-note"
    }
  },
  {
    "name": "Talebook",
    "root_path": "projects/talebook/frontend",
    "settings": {
      "path": ".,src/**,,",
      "suffixesadd": ".js,.vue",
      "commentstring": "// %s",
      "includeexpr": "tolower(v:fname)"
    },
    "regex_jump": {
      "T": {
        "from": "src/(.*)\\.(js|vue)",
        "to": "test/unit/{}.js"
      }
    },
    "files": {
      "c": "src/components",
      "s": "src/services",
      "t": "src/stores"
    }
  },
  {
    "name": "Reactapp",
    "root_path": "projects/reactapp",
    "dictionary": "~/.config/nvim/dict/react",
    "settings": {
      "path": ".,src/**,,",
      "suffixesadd": ".js"
    },
    "files": {
      "c": "src/components"
    }
  },
  {
    "name": "Ixl",
    "root_path": "projects/ixl-front-end",
    "dictionary": "~/.config/nvim/dict/react",
    "settings": {
      "shiftwidth": 4,
      "softtabstop": 4,
      "tabstop": 4,
      "path": ".,,src/**",
      "suffixesadd": ".js"
    },
    "files": {
      "c": "src/components",
      "r": "src/redux",
      "v": "src/views"
    }
  }
]
